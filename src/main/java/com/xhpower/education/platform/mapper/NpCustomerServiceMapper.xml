<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xhpower.education.platform.dao.NpCustomerServiceMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.xhpower.education.platform.entity.NpCustomerService">
		<id column="id" property="id" />
		<result column="qq" property="qq" />
		<result column="status" property="status" />
		<result column="status_value" property="statusValue" />
		<result column="remarks" property="remarks" />
		<result column="create_time" property="createTime" />
		<result column="update_time" property="updateTime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, qq, status, status_value AS statusValue, remarks, create_time AS createTime, update_time AS updateTime
    </sql>

</mapper>
